<!-- TODO what is {{ ind }}? Look for it in backbone implementation -->
<input type="radio"
    ng-repeat="el in field.values.radioitems"
    id="{{ getName(field.input) }}_{{ ind }}"
    name="{{ getName(field.input) }}"
    value="{{ el.id }}"
    ng-required="field.mandatory"
    ng-disabled="!hasPlacesLeft(field)"/>
{{ el.caption }}

<span class="inputDescription">
    {{ field.description }}
</span>
