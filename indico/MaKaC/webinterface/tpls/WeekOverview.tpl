<table width="100%%" cellspacing="0" cellpadding="0" align="center">
    <tr>
        <td style="text-align: center; padding-bottom: 10px;">
            <em style="font-size: 1.3em;"><%= dates %></em>
        </td>
    </tr>
    <tr>
        <td align="center">
            <table width="100%%" style="border: 1px solid #AAA;" cellspacing="1" cellpadding="3">
                <tr>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date0)s
                    </td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date1)s
                    </td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date2)s
                    </td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date3)s
                    </td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date4)s
                    </td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date5)s
                    </td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>
                        %(date6)s
                    </td>
                </tr>
                <tr>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                                %(item0)s
                        </table>
                    </td>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                            %(item1)s
                        </table>
                    </td>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                            %(item2)s
                        </table>
                    </td>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                            %(item3)s
                        </table>
                    </td>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                            %(item4)s
                        </table>
                    </td>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                            %(item5)s
                        </table>
                    </td>
                    <td bgcolor="#ECECEC" valign="top">
                        <table width="100%%">
                            %(item6)s
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
