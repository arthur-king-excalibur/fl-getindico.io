<table width="100%%" cellspacing="0" cellpadding="0" align="center">
    <tr>
        <td style="text-align: center; padding-bottom: 10px;">
            <em style="font-size: 1.3em;"><%= dates %></em>
        </td>
    </tr>
    <tr>
        <td align="center">
            <table width="100%%" style="border: 1px solid #AAA;" cellspacing="1" cellpadding="3">
                <tr>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay0)s</td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay1)s</td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay2)s</td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay3)s</td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay4)s</td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay5)s</td>
                    <td style="background-color: #444; color: white; text-align: center" nowrap>%(nameDay6)s</td>
                </tr>
                %(items)s
            </table>
        </td>
    </tr>
</table>
